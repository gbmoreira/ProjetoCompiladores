programa teste
	inteiro i j
	logico sim
inicio 
	i<-1
	enquanto nao(i<10) faca
		j<-1
		enquanto nao(j<sim) faca
			escreva i + j
			j<- j+1
			
			enquanto j<5 faca
				se nao(sim) entao
					escreva i+9
				senao
					escreva i+1
				fimse
			fimenquanto
	

		fimenquanto
		i<-4
	fimenquanto

fimprograma
